  @updateSelected={{@updateSelectedGovernment}}
  @noMatchesMessage="Geen gemeentes gevonden"
  @searchField="label"
  @placeholder="Kies één of meerdere gemeentes"
  @queryParam="gemeentes+provincies"
  @searchEnabled={{true}}
/>