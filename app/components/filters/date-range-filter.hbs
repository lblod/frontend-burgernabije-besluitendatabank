<Filters::Filter @id={{@id}} @label={{@label}}>
    <AuDateRangePicker
      @width="block"

      id={{@id}}
      @label={{@label}}
      @start={{@valueA}}
      @end={{@valueB}}
      
      @cancelAction={{action this.resetDate}}
      @applyAction={{action this.dateChange}}
      @hideAction={{action this.dateChange}}

      @cancelLabel="Reset"
    />
</Filters::Filter> 
